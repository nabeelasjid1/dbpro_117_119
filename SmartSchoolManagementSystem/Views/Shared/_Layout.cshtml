                                @Html.Partial("_LoginPartial")

                            </ul>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-area header-sticky fixed">
            <div class="container">
                <div class="row">
                    <div class="col-md-3 col-sm-3 col-xs-6">
                        <div class="logo">
                            <a href="@Url.Action("Index","Home")"><img src="~/Content/img/logo/logo.png" alt="eduhome" /></a>
                        </div>
                    </div>
                    <div class="col-md-9 col-sm-9 col-xs-12">
                        <div class="content-wrapper one">
                            <!-- Main Menu Start -->
                            <div class="main-menu one text-right">
                                <nav>
                                    <ul>
                                        <li>
                                            <a href="@Url.Action("Index","Home")">Home</a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("Index","Course")">courses</a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("Index","Event")">events</a>
                                        </li>
                                        <li class="hidden-sm">
                                            <a href="@Url.Action("Index","Faculty")">Our Faculty</a>
                                        </li>
                                        <li>
                                            <a href="#"> Dashboard</a>
                                            <ul>
                                                <li><a href="@Url.Action("Index", "Admin")">Admin</a></li>
                                                <li><a href="@Url.Action("Index", "Teacher")">Teacher</a></li>
                                                <li><a href="@Url.Action("Index", "Student")">Student</a></li>
                                                <li><a href="@Url.Action("Index", "Parent")">Parent</a></li>
                                            </ul>
                                        </li>
                                    </ul>
                                </nav>
                            </div>
                            <div class="mobile-menu hidden-lg hidden-md one"></div>
                            <!-- Main Menu End -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    @RenderBody()

