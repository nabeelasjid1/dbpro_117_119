                            <h3>useful links</h3>
                            <ul>
                                <li><a href="@Url.Action("Index","Course")">our courses</a></li>
                                <li><a href="#">About Us</a></li>
                                <li><a href="@Url.Action("Index","Faculty")">teachers &amp; faculty</a></li>
                                <li><a href="#">terms &amp; conditions</a></li>
                                <li><a href="@Url.Action("Index","CEvent")">our events</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6 col-xs-12">
